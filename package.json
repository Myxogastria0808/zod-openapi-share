{
  "name": "zod-openapi-share",
  "version": "0.0.1",
  "description": "This package works the wrapper of @hono/zod-openapi.",
  "keywords": [
    "@hono/zod-openapi",
    "hono",
    "zod",
    "openapi",
    "typescript",
    "wrapper"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pub": "pnpm publish --access=public",
    "unpub": "pnpm unpublish  zod-openapi-share@${npm_config_v}",
    "build": "tsup",
    "typedoc": "pnpm dlx typedoc",
    "test": "vitest run --typecheck",
    "coverage": "vitest run --coverage --typecheck",
    "ui": "vitest run --ui --typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Myxogastria0808/zod-openapi-share.git"
  },
  "author": "Yuki Osada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Myxogastria0808/zod-openapi-share/issues"
  },
  "homepage": "https://myxogastria0808.github.io/zod-openapi-share/",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@hono/zod-openapi": "^1.1.3",
    "hono": "^4.9.9"
  },
  "peerDependencies": {
    "@hono/zod-openapi": "^1.1.3",
    "hono": "^4.9.9"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
